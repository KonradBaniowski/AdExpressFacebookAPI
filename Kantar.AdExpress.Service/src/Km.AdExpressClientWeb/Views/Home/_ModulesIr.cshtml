                                <i class="icon-chart sli-size blue"></i>
                                <h3>@Model.Modules[(Module.Name.ANALYSE_PLAN_MEDIA)].Title</h3>
                            </div>
                            <div class="back">
                                <p class="backText font-size12 text-center">@Model.Modules[(Module.Name.ANALYSE_PLAN_MEDIA)].Description</p>
                            </div>
                        </div>
                    </a>
            }
