@model IEnumerable<Km.AdExpressClientWeb.Models.SocialMedia.KPIPageFacebookContract>



<div class="col-lg-12 col-md-12 col-sm-12">
    @foreach (var month in Model)
    {
        <div class="col-lg-4 text-center elmtsChart">
            @Html.Hidden(month.Month, null, new { @class = "monthRef" })
            @Html.Hidden(month.Comment.ToString(), null, new { @class = "commentRef" })
            @Html.Hidden(month.Like.ToString(), null, new { @class = "likeRef" })
            @Html.Hidden(month.Share.ToString(), null, new { @class = "shareRef" })
            @Html.Hidden(month.Post.ToString(), null, new { @class = "postRef" })
        </div>
    }

</div>
<div class="col-lg-12 col-md-12 col-sm-12  margin-top-30">
    <div class="col-lg-4">
        <div id="chart" class="div-center">

        </div>
    </div>
</div>
